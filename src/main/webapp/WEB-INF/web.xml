<web-app>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
	


	<context-param>
			<param-name>url</param-name>
			<param-value>jdbc:mysql://localhost:3306/tapacadmey</param-value>
	</context-param>
	<context-param>
			<param-name>username</param-name>
			<param-value>root</param-value>
	</context-param>
	<context-param>
			<param-name>password</param-name>
			<param-value>Jslc@123</param-value>
	</context-param>
	
	


	<servlet>
		<servlet-name>Servlet1</servlet-name>
		<servlet-class>com.J2EE.servlets.FirstServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Servlet1</servlet-name>
		<url-pattern>/Serv1</url-pattern>
	</servlet-mapping>





	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.J2EE.student.validation</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	


	<servlet>
		<servlet-name>drive</servlet-name>
		<servlet-class>com.J2EE.student.drive</servlet-class>
		
	</servlet>
	
	<servlet-mapping>
		<servlet-name>drive</servlet-name>
		<url-pattern>/drive</url-pattern>
	</servlet-mapping>




	<servlet>
		<servlet-name>eligble</servlet-name>
		<servlet-class>com.J2EE.student.eligble</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>eligble</servlet-name>
		<url-pattern>/eligble</url-pattern>
	</servlet-mapping>


</web-app>